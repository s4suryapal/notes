{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start --dev-client --lan",
    "dev:tunnel": "EXPO_NO_TELEMETRY=1 expo start --dev-client --tunnel",
    "build:web": "expo export --platform web",
    "build:apk": "cd android && ./gradlew assembleRelease && cd ..",
    "build:dev-apk": "cd android && ./gradlew assembleDebug && cd ..",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "android": "expo run:android",
    "android:dev": "expo run:android --variant debug",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^23.4.0",
    "@react-native-firebase/app": "^23.4.0",
    "@react-native-firebase/crashlytics": "^23.4.0",
    "@react-native-firebase/messaging": "^23.4.0",
    "@react-native-firebase/perf": "^23.4.0",
    "@react-native-firebase/remote-config": "^23.4.0",
    "@react-native-ml-kit/text-recognition": "^2.0.0",
    "@react-navigation/drawer": "^7.5.8",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.58.0",
    "expo": "^54.0.10",
    "expo-av": "^16.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.13",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image-crop-tool": "^0.3.1",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-local-authentication": "~17.0.7",
    "expo-notifications": "^0.32.12",
    "expo-print": "^15.0.7",
    "expo-router": "~6.0.8",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "fuse.js": "^7.1.0",
    "lottie-react-native": "^7.3.4",
    "lucide-react-native": "^0.544.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-document-scanner-plugin": "^2.0.2",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^15.8.0",
    "react-native-mmkv": "^3.3.3",
    "react-native-pager-view": "^6.9.1",
    "react-native-pell-rich-editor": "^1.10.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-tab-view": "^4.1.3",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-view-shot": "^4.0.3",
    "react-native-web": "^0.21.0",
    "reanimated-color-picker": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "eslint": "^9.37.0",
    "eslint-config-expo": "^10.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  }
}
